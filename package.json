{
  "name": "screenity",
  "version": "3.0.0",
  "description": "The free and privacy-friendly screen recorder with no limits",
  "scripts": {
    "build": "node utils/build.js",
    "build:webapp": "webpack --config webpack.webapp.config.js",
    "start": "node utils/server.js",
    "start:webapp": "webpack serve --config webpack.webapp.config.js",
    "dev": "concurrently \"npm run start\" \"npm run build:watch\"",
    "dev:webapp": "npm run start:webapp",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "postinstall": "patch-package"
  },
